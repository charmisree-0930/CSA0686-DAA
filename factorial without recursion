#include<stdio.h>  
int main()    
{    
 int i,fact=1,number;    
 printf("Enter a number: ");    
  scanf("%d",&number);    
    for(i=1;i<=number;i++){    
      fact=fact*i;    
  }    
  if(number<0)
  {
  	printf("invalid input");
  }
  else
  {
  printf("Factorial of %d is: %d",number,fact);   
  }
return 0;  
}
